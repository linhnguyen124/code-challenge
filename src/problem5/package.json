{
	"name": "problem5_service",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "nodemon -x 'npx ts-node ./src/startup/app.ts' -w src -w .env -w package.json -e ts,js",
		"docs": "nodemon ./docs/index.js -w docs",
		"generate:jwt": "node -e \"console.log(require('crypto').randomBytes(32).toString('hex'))\"",
		"build:tsc": "rm -rf build && tsc && tsc-alias",
		"migrate:all": "sequelize db:migrate",
		"seed:all": "sequelize-cli db:seed:all",
		"db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
	},
	"keywords": [],
	"author": "PhanVanBinh",
	"license": "PhanVanBinh",
	"description": "Problem5 Service",
	"dependencies": {
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"formidable": "^3.5.2",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"lodash": "^4.17.21",
		"morgan": "^1.10.0",
		"pg": "^8.12.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.37.3",
		"swagger-ui-express": "^5.0.1",
		"type-is": "^1.6.18",
		"uuid": "^10.0.0",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
		"yamljs": "^0.3.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/formidable": "^3.4.5",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/lodash": "^4.17.7",
		"@types/morgan": "^1.9.9",
		"@types/type-is": "^1.6.7",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^8.2.0",
		"@typescript-eslint/parser": "^8.2.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.2.1",
		"nodemon": "^3.1.4",
		"prettier": "^3.3.3",
		"prettier-eslint": "^16.3.0",
		"sequelize-cli": "^6.6.2",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.10",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.18.0",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.2.0"
	}
